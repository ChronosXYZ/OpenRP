            format(str6, sizeof(str6), "~b~MODEL: ~w~%s", VehicleNames[GetVehicleModel(caridi)-400]);
            format(str4, sizeof(str4), "~b~FUEL: ~g~%s%.0f", vehs, Fuell[caridi]);
			format(str5, sizeof(str5), "%s", vehe);
