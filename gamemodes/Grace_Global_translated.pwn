publics UpdateSpeedometr()
{
    foreach(new i : Player)
	{
    	if(IsPlayerInAnyVehicle(i))
		{
    		new vehicleid = GetPlayerVehicleID(i);
			new locked[32];
			new Float: vehhel, vehh[20], vehs[20], vehe[20];
	    	new caridi = GetPlayerVehicleID(i);
	    	if(IsLocked[caridi] == 1) locked = "~b~VEHICLE: ~r~LOCK";
            else locked = "~b~VEHICLE: ~g~UNLOCK";
	    	GetVehicleHealth(GetPlayerVehicleID(i), vehhel);
	    	if(vehhel <= 500) vehh = "~r~";
            else vehh = "";
            if(Fuell[caridi] < 50) vehs = "~r~";
	    	else vehs = "";
