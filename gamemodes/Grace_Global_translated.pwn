	{
		if(robh[playerid] != true) return SCM(playerid,COLOR_GREY,"You don't have materials to submit");
		{
			robh[playerid] = false;
			PlayerInfo[playerid][pCash] += 500;
			SCM(playerid,COLOR_GREY,"You handed over the robbery for 500$");
			return true;
		}
	}
	else if(pickupid == collection)
    {
        if(CollectionStarted[playerid] == 2)
        {
            if(FarmWorks[playerid] == 1)
            {
                if(WithBush[playerid] == 1)
                {
                    for(new f = 0; f < sizeof(FarmInfo); f++)
                    {
                        if(FarmInfo[f][fInCar] >= 1000)
