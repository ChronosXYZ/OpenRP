publics StartRace()
{
	new string[61];
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i))
		{
			if(PlayerRacing[i] != 0)
 			{
				RaceCP[i] = 10;
				GameTextForPlayer(i, "~r~GO! GO! GO!", 5000, 3);
				PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
				chet[i] += 1;
				SetPlayerRaceCheckpoint(i,1,race[0][0],race[0][1],race[0][2], race[1][0],race[1][1],race[1][2],8.0);
				TogglePlayerControllable(i, 1);
				PlayerRacing[i] = 1;
				GetVehicleParamsEx(GetPlayerVehicleID(i), engine,lights,alarm,doors,bonnet,boot,objective);
				SetVehicleParamsEx(GetPlayerVehicleID(i), 1,1,alarm,doors,bonnet,boot,objective);
				EngineState[i] = true;
			}
		}
		format(string, sizeof(string), "Attention! The race has begun. Route: Los Santos Airport");
		SCMTA(0xB9B900AA,string);
		SetTimer("RaceEnded", 360000, 0);
		EndingRaceRound = 1;
		RacingPlayers = 0;
	}
	return true;
}
publics RaceEnded()
{
    foreach(new i : Player)
	{
    	if(IsPlayerConnected(i))
		{
   			new string[62];
  			RaceCP[i] = 0;
  			DisablePlayerCheckpoint(i);
  			format(string, sizeof(string),"Attention! The race is over. (( List of winners: /racelist ))");
  			SCMTA(0xB9B900AA,string);
  			PlayerRacing[i] = 0;
  			EndingRaceRound = 1;
  			RacingPlayers = 0;
			if(PlayerRacing[i] != 0) DestroyVehicle(GetPlayerVehicleID(i));
	 	}
	}
}
