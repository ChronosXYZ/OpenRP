                            FarmInfo[f][fInCar] = 1000;
                            DisablePlayerCheckpoint(playerid);
                            format(string, sizeof(string), "Uploaded\n{ffffff}%d / 1000", FarmInfo[f][fInCar]);
                            Update3DTextLabelText(JobText2, 0x5CABBDAA, string);
                            return true;
                        }
                        else
                        {
                            FarmInfo[f][fSownSeeds] -= 5;
                            FarmInfo[f][fInCar] += 5;
                            PlayerInfo[playerid][pFermazarobotok] += FarmInfo[f][fEntranceCost];
                            format(string, sizeof(string), "~b~count:~w~+%d$",PlayerInfo[playerid][pFermazarobotok]);
                            GameTextForPlayer(playerid, string, 3000, 1);
